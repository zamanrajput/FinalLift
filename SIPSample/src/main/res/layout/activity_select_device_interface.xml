<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color_admin"
    android:orientation="vertical"
    android:padding="@dimen/_8sdp">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="start"
        android:layout_marginTop="@dimen/_10sdp"
        android:gravity="center">

        <androidx.cardview.widget.CardView
            android:id="@+id/backBtn"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_35sdp"
            app:cardBackgroundColor="@color/card_btn_bg_del"
            app:cardCornerRadius="@dimen/_6sdp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:layout_gravity="center"
                    android:src="@drawable/left_arrow" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_4sdp"
                    android:text="GO BACK"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />
            </LinearLayout>


        </androidx.cardview.widget.CardView>


    </LinearLayout>

    <ImageView
        android:id="@+id/submit"
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/_10sdp"
        android:src="@drawable/save_ic" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:padding="@dimen/_16sdp">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingHorizontal="@dimen/_20sdp"
                android:paddingTop="@dimen/_10sdp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="@dimen/_200sdp"
                        android:layout_height="wrap_content"
                        android:text="Machine Configuration"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10sdp" />

                    <LinearLayout
                        android:id="@+id/deviceIDLayout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"

                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/et"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Device ID"
                            android:inputType="textPersonName"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:gravity="center">

                        <Spinner
                            android:id="@+id/devices_spinner"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center"
                            android:inputType="number"
                            android:textAllCaps="true"
                            android:textColor="@color/et_text_field_color"
                            android:textColorHint="@color/et_text_field_color_hint"
                            android:textSize="@dimen/_18sdp"
                            tools:listitem="@layout/device_interface_item" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:gravity="center">

                        <Spinner
                            android:id="@+id/ratesSpinner"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center"
                            android:inputType="number"
                            android:textAllCaps="true"
                            android:textColor="@color/et_text_field_color"
                            android:textColorHint="@color/et_text_field_color_hint"
                            android:textSize="@dimen/_20sdp"
                            android:textStyle="bold"
                            tools:listitem="@layout/device_interface_item" />
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"

                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/helpline"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Help Line Number"
                            android:inputType="textPersonName"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="@dimen/_200sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:text="SIP Configuration"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10sdp" />

                    <LinearLayout
                        android:id="@+id/etUserNameLayout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/username"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Username"
                            android:inputType="textPersonName"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/password"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Password"
                            android:inputType="textPersonName"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/serverIP"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Server Address"
                            android:inputType="textPersonName"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:gravity="center">

                        <EditText
                            android:id="@+id/port"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/selected_field_bg"
                            android:gravity="start|center"
                            android:hint="Process Port"
                            android:inputType="number"
                            android:paddingHorizontal="@dimen/_10sdp"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/tint_black"
                            android:textSize="@dimen/_13sdp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginHorizontal="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:gravity="center">

                        <Spinner
                            android:id="@+id/protocolSpinner"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null"
                            android:gravity="center"
                            android:inputType="number"
                            android:textAllCaps="true"
                            android:textColor="@color/et_text_field_color"
                            android:textColorHint="@color/et_text_field_color_hint"
                            android:textSize="@dimen/_18sdp"
                            tools:listitem="@layout/device_interface_item" />
                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>
        </androidx.core.widget.NestedScrollView>


